<h1>Search a movie</h1>

<form action="/movie-search" method="GET">
  <input type="text" placeholder="search term" name="search">
  <input type="submit">
</form>

<h1>Search Result</h1>

<% if (data != null) { %>
  <ul>
      <% data["Search"].forEach(function(movie){ %>
        <li> 
        <strong><%= movie["Title"] %> </strong> 
        - <%= movie["Year"] %>
        </li>
      <% }) %>
  </ul>
<% } %>
